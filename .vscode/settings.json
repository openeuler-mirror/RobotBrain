    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "set": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "slist": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "cinttypes": "cpp",
    "typeinfo": "cpp",
    "filesystem": "cpp",
    "csignal": "cpp",
    "strstream": "cpp",
    "bitset": "cpp",
    "chrono": "cpp",
    "complex": "cpp",
    "ratio": "cpp",
    "thread": "cpp",
    "cfenv": "cpp",
    "typeindex": "cpp",
    "cholesky": "cpp",
    "core": "cpp",
    "eigenvalues": "cpp",
    "geometry": "cpp",
    "householder": "cpp",
    "jacobi": "cpp",
    "lu": "cpp",
    "qr": "cpp",
    "svd": "cpp",
    "stdvector": "cpp",
    "any": "cpp",
    "codecvt": "cpp",
    "condition_variable": "cpp",
    "regex": "cpp",
    "future": "cpp",
    "mutex": "cpp",
    "scoped_allocator": "cpp",
    "valarray": "cpp",
    "variant": "cpp",
    "*.idl": "cpp",
    "*.inc": "cpp",
    "*.in": "plaintext"
  },
  "C_Cpp.default.configurationProvider": "go2sh.cmake-integration",
  "cmake.buildDirectory": "${workspaceFolder}/out/build",
  "gitlab.instanceUrl": "https://gitlab.rosc.org.cn",
  "editor.detectIndentation": false,
  "editor.rulers": [
    80
  ],
  "doxdocgen.file.copyrightTag": [
    "@copyright Copyright (c) {year} ROSC"
  ],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "modifications",
  "[plaintext]": {
    "editor.formatOnSave": false
  },
  "editor.tabSize": 2,
  "cmake.copyCompileCommands": "${workspaceFolder}/compile_commands.json",
  "cpplint.excludes": [
    "3rdparty/",
    "out/"
  ],
  "git.ignoreLimitWarning": true,
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_style": "Google",
  "cmake.configureOnOpen": true,
  "files.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    // "3rdparty/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "3rdparty/**": true
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)(\\n\\s*//\\s{2,}.*)*",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "DEPRECATED",
    "WARNING",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regexCaseSensitive": false,
  "todo-tree.highlights.defaultHighlight": {
    "type": "text",
    "foreground": "#ffff00",
    "background": "#aaa",
    "opacity": 0.5,
    "iconColour": "#ffff00",
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "background": "#01ffc8",
      "icon": "checkbox",
      "iconColour": "#01ffc8",
      "type": "whole-line"
    },
    "FIXME": {
      "background": "#ff00ff",
      "icon": "alert",
      "iconColour": "#ff00ff",
      "type": "whole-line"
    },
    "BUG": {
      "icon": "bug",
      "background": "#ff0000",
      "iconColour": "#ff0000",
      "type": "whole-line"
    },
    "HACK": {
      "icon": "code",
      "background": "#ffff00",
      "iconColour": "#ffff00",
      "type": "whole-line"
    },
    "DEPRECATED": {
      "icon": "issue-draft",
      "background": "#ffab03",
      "iconColour": "#ffab03",
      "type": "whole-line"
    },
  },
}